   "source": [
    "#6-1\n",
    "import numpy as np\n",
    "from scipy.integrate import odeint\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "Ti = 80\n",
    "ts = 25\n",
    "cp = 1\n",
    "thou = 1000\n",
    "v = 1000\n",
    "ua = 5714\n",
    "ti = 0\n",
    "te = 1\n",
    "temp0 = 60\n",
    "tt = [0]\n",
    "yy = [60]\n",
    "temp = [60,60,60,60]\n",
    "temps = 63\n",
    "kc = 10\n",
    "\n",
    "def heating(temp, t):\n",
    "    global f\n",
    "    dydt = (f * thou * cp * Ti - ua * (temp - ts) - f * thou * cp * temp) / (v * cp * thou)\n",
    "    return dydt\n",
    "# Main program\n",
    "\n",
    "\n",
    "for i in range(100):\n",
    "    f = 10 + kc * (temps - temp[3])\n",
    "    t = np.linspace(ti, te, 50)\n",
    "    sol = odeint(heating, temp[0], t)\n",
    "    \n",
    "    for j in range(3):\n",
    "        temp[3-j] = temp[2-j]\n",
    "    temp[0] = sol[-1]\n",
    "    ti = te\n",
    "    te += 1\n",
    "    tt.append(ti)\n",
    "    yy.append(temp[0])\n",
    "\n",
    "def heating2(Y, t):\n",
    "    dy = (f2(Y)*thou*cp*Ti-ua*(Y-ts)-f2(Y)*thou*cp*Y)/(v*cp*thou)\n",
    "    return dy\n",
    "\n",
    "def f2(Y):\n",
    "    yset = 63\n",
    "    kc2 = 10\n",
    "    return 10+kc2*(yset-Y)\n",
    "\n",
    "# Time span\n",
    "t = np.linspace(0, 100, 100)\n",
    "Y = odeint(heating2, 60, t)\n",
    "\n",
    "plt.plot(t, Y, label='T(t)', color='red')\n",
    "plt.plot(tt,yy)\n",
    "plt.xlabel('Time')\n",
    "plt.ylabel('Temperature')\n",
    "plt.title('Temperature vs. Time')\n",
    "plt.grid(True)\n",
    "plt.show()\n"
   ]
