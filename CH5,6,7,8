   "source": [
    "import numpy as np\n",
    "from scipy.integrate import odeint\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "def pi_example(y, t, kc, taui):\n",
    "    ys = 1\n",
    "    e = ys - y[0]\n",
    "    u = kc * (e + y[1] / taui) \n",
    "    # y[1] is the integral of E(t)\n",
    "    dy1 = (3 * u - y[0]) / 2\n",
    "    dy2 = e\n",
    "    return [dy1, dy2]\n",
    "def p(y,t):\n",
    "    ys = 1\n",
    "    kc = 1\n",
    "    u = kc*(ys-y)\n",
    "    dydt = (-y+3*u)/2\n",
    "    return dydt\n",
    "t = np.linspace(0, 30, 100)\n",
    "p = odeint(p,0,t)\n",
    "y0 = [0, 0]\n",
    "kc = 1\n",
    "\n",
    "def y(taui):\n",
    "    return odeint(pi_example, y0, t, args=(kc, taui))\n",
    "\n",
    "plt.plot(t, y(3)[:,0], 'r')\n",
    "plt.plot(t, y(0.5)[:,0],'b')\n",
    "plt.plot(t, y(1)[:,0],'g')\n",
    "plt.plot(t,p,'o')\n",
    "plt.xlabel('Time')\n",
    "plt.ylabel('y')\n",
    "plt.title('Response of PI Controller')\n",
    "plt.grid(True)\n",
    "plt.show()\n"
   ]
  },
