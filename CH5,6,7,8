   "source": [
    "import numpy as np\n",
    "from scipy.integrate import odeint\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Define the thermal1 function\n",
    "def thermal1(temp, t):\n",
    "    global f\n",
    "    ti = 80\n",
    "    ts = 25\n",
    "    cp = 1\n",
    "    thou = 1000\n",
    "    v = 1000\n",
    "    ua = 5714\n",
    "    dydt = (f(temps,temp0) * thou * cp * ti - ua * (temp - ts) - f(temps,temp0) * thou * cp * temp) / (v * cp * thou)\n",
    "    return dydt\n",
    "\n",
    "# Main program\n",
    "t_sample = 1 #模擬時間步長\n",
    "temps = 63\n",
    "ti = 0\n",
    "te = t_sample\n",
    "temp0 = 60\n",
    "t_save = [ti] #創一個時間序列，放進第一個時間點\n",
    "f_save = [10]  # 創一個時間序列，放進f的初始值(題目給的)\n",
    "time = [] #創一個空列以容納迴圈增加的時間點\n",
    "temp_save = [temp0] #創一個溫度序列，放進溫度初始值(題目給的)\n",
    "kc = 3\n",
    "n = 50\n",
    "#定義f函數\n",
    "def f(temps,temp0):\n",
    "    return kc * (temps - temp0) + 10\n",
    "#迴圈，range內放你要畫的總時間長\n",
    "for i in range(n):\n",
    "    t = np.linspace(ti, te, 100) \n",
    "    #生成一个包含100个时间点的数组，这些时间点均匀分布在ti和te之间，其中ti是当前时间，te是当前时间加上t_sample。\n",
    "    sol = odeint(thermal1, temp0, t)\n",
    "    temp0 = sol[-1] #把下一個解的溫度初始值校正為此次計算的結果(序列最末項)\n",
    "\n",
    "    # Append results\n",
    "    t_save.append(t[-1]) #儲存此次計算結果\n",
    "    temp_save.append(temp0)\n",
    "    f_save.append(f(temps,temp0))  # Append the updated control signal\n",
    "\n",
    "    # Update time for next iteration\n",
    "    ti = te\n",
    "    te += t_sample\n",
    "\n",
    "#以下三行非必要，直接用序列也行，但python不推薦  \n",
    "t_save = np.array(t_save, dtype=object) #把t_save轉成array，用dtype = object是為了確保序列內每個元素都是python對象，但也是非必要\n",
    "temp_save = np.array(temp_save, dtype=object)\n",
    "f_save = np.array(f_save, dtype=object)\n",
    "\n",
    "# Plotting\n",
    "plt.plot(t_save, f_save, marker='o', linestyle='-')\n",
    "plt.step(t_save, f_save)\n",
    "\n",
    "plt.xlabel('Time')\n",
    "plt.ylabel('Control Signal')\n",
    "plt.title('Control Signal vs. Time')\n",
    "plt.grid(True)\n",
    "plt.show()\n"
   ]
