    "#8-4\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "from scipy.integrate import odeint\n",
    "from scipy.optimize import root\n",
    "\n",
    "def func(h,t):\n",
    "    a1 = 10\n",
    "    a2 = 10\n",
    "    k1 = 1.5\n",
    "    k2 = 1.5\n",
    "    fi = 4\n",
    "    fo = 0.8\n",
    "    f1  = k1*np.sqrt(h[0])\n",
    "    f2 = k2*np.sqrt(h[1])\n",
    "    dh1 = (fi-fo-f1)/a1\n",
    "    dh2 = (f1-f2)/a2\n",
    "    return [dh1,dh2]\n",
    "t = np.linspace(0,250,100)\n",
    "y= odeint(func,[4,4],t)\n",
    "# 定义时间点\n",
    "tt = np.linspace(0, 250)\n",
    "\n",
    "# 计算 h2\n",
    "h2 = 4 + 2.667 * 0.2 * (1 - (1 + tt / 26.667) * np.exp(-tt / 26.667))\n",
    "\n",
    "# 绘图\n",
    "plt.plot(tt, h2,color = 'r',label = 'linear')\n",
    "plt.plot(t,y[:,1], color = 'b', label = 'nonlinear')\n",
    "plt.legend()"
   ]
