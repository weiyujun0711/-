   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "from scipy.integrate import odeint\n",
    "from scipy.optimize import root\n",
    "fa = 4\n",
    "fb = 1\n",
    "fr = 0.5\n",
    "v1 = 10\n",
    "v2 = 20\n",
    "fc = fa + fr\n",
    "t1 = 80\n",
    "t3 = 40\n",
    "def func(y,t):\n",
    "    t2 = y[0]\n",
    "    t4 = y[1]\n",
    "    tau1 = v1/(fa+fr)\n",
    "    tau2 = v2/(fc+fb)\n",
    "    dy1 = (fa/(fa + fr)*t1 +fr/(fa+fr)*t4-t2)/tau1\n",
    "    dy2 = (fc/(fc+fb)*t2+fb/(fc+fb)*t3-t4)/tau2\n",
    "    return [dy1,dy2]\n",
    "def yy(t):\n",
    "    return 64 + 8*(1-(-2*np.exp(-t/2)+4.4*np.exp(-t/4.4))/(4.4-2))\n",
    "t = np.linspace(0,50,100)\n",
    "yode = odeint(func,[69.3333,64],t)\n",
    "y2 = yy(t)\n",
    "plt.plot(t,yode[:,1],label = 'original function') #畫的是T4\n",
    "plt.plot(t,y2, color = 'r',label = 'Analytical solution')\n",
    "plt.legend()"
   ]
