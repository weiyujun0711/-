   "cell_type": "code",
   "execution_count": null,
   "id": "30e067dd",
   "metadata": {},
   "outputs": [],
   "source": [
    "#6-2\n",
    "import numpy as np\n",
    "from scipy.integrate import odeint\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Define the thermal1 function\n",
    "def func(y, t):\n",
    "    global u\n",
    "    dy = (2*u-y)/3\n",
    "    return dy.flatten()\n",
    "\n",
    "# Main program\n",
    "t_sample = 1\n",
    "ti = 0\n",
    "te = t_sample\n",
    "y0 = 0\n",
    "t_save = [ti] #創一個時間序列，放進第一個時間點\n",
    "u_save = [0]  # 創一個時間序列，放進f的初始值(題目給的)\n",
    "time = [] #創一個空列以容納迴圈增加的時間點\n",
    "y_dis = [0]\n",
    "y_save = [y0] #創一個溫度序列，放進溫度初始值(題目給的)\n",
    "\n",
    "\n",
    "#定義f函數\n",
    "#迴圈，range內放你要畫的總時間長\n",
    "for i in range(30):\n",
    "    u = np.random.randn(1,1)\n",
    "    t = np.linspace(ti, te, 100) \n",
    "    yd = 0.7165*y0 + 0.5669*u\n",
    "    #生成一个包含100个时间点的数组，这些时间点均匀分布在ti和te之间，其中ti是当前时间，te是当前时间加上t_sample。\n",
    "    sol = odeint(func, y0, t)\n",
    "    y0 = sol[-1] #把下一個解的溫度初始值校正為此次計算的結果(序列最末項)\n",
    "\n",
    "    # Append results\n",
    "    y_dis.append(yd)\n",
    "    t_save.append(t[-1]) #儲存此次計算結果\n",
    "    y_save.append(y0)\n",
    "    u_save.append(u)  # Append the updated control signal\n",
    "\n",
    "    # Update time for next iteration\n",
    "    ti = te\n",
    "    te += t_sample\n",
    "\n",
    "#以下三行非必要，直接用序列也行，但python不推薦  \n",
    "t_save = np.array(t_save, dtype=object) #把t_save轉成array，用dtype = object是為了確保序列內每個元素都是python對象，但也是非必要\n",
    "y_save = np.array(y_save, dtype=object)\n",
    "u_save = np.array(u_save, dtype=object)\n",
    "y_dis = np.array(y_dis, dtype = object)\n",
    "# Plotting\n",
    "plt.plot(t_save, u_save, marker='o', linestyle='-')\n",
    "plt.plot(t_save, y_save,color = 'r')\n",
    "plt.plot(t_save,y_dis,marker = '+',color = 'g')\n",
    "plt.xlabel('Time')\n",
    "plt.ylabel('Control Signal')\n",
    "plt.title('Control Signal vs. Time')\n",
    "plt.grid(True)\n",
    "plt.show()\n"
   ]
  },
