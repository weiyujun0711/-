   "source": [
    "#脈衝波\n",
    "import numpy as np\n",
    "from scipy.integrate import odeint\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# 定义脉冲变化的ODE模型\n",
    "def model(y, t):\n",
    "    if 0 <= t < 1:\n",
    "        dydt = 1 - y\n",
    "    else:\n",
    "        dydt = -y\n",
    "    return dydt\n",
    "\n",
    "# 初始条件\n",
    "y0 = 0\n",
    "\n",
    "# 时间范围\n",
    "t = np.linspace(0, 5, 100)\n",
    "\n",
    "# 求解ODE\n",
    "y = odeint(model, y0, t)\n",
    "t2 = np.linspace(-2,0,100)\n",
    "y2 = odeint(model,y0, t2)\n",
    "# 绘制图形\n",
    "plt.plot(t, y, label='y(t)', color =\"blue\")\n",
    "plt.plot(t2, y2, color = \"blue\")\n",
    "plt.xlabel('Time (t)')\n",
    "plt.ylabel('y')\n",
    "plt.title('Solution of the Pulse Change ODE')\n",
    "plt.legend()\n",
    "plt.grid(True)\n",
    "plt.show()\n"
   ]
  },
