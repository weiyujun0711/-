    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "from scipy.integrate import odeint\n",
    "\n",
    "def func(y,t):\n",
    "    dydt = u(t) - y\n",
    "    return dydt\n",
    "def u(t):\n",
    "    if t >= 0:\n",
    "        return np.exp(-t)\n",
    "    else:\n",
    "        return 0\n",
    "t = np.linspace(-2,10,100)\n",
    "y = odeint(func,0, t)\n",
    "plt.plot(t,y)\n",
    "plt.plot(t,[u(ti) for ti in t])\n",
    "plt.show()"
   ]
