   "source": [
    "import numpy as np\n",
    "from scipy.integrate import odeint\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Define the pulse-change ODE model\n",
    "def model(y, t):\n",
    "    if 0 <= t < 1:\n",
    "        dydt = 1 - y\n",
    "    else:\n",
    "        dydt = -y\n",
    "    return dydt\n",
    "\n",
    "# Calculate the initial condition based on the function at t = -2\n",
    "initial_condition = 1 - np.exp(0.000001)\n",
    "\n",
    "# Time range\n",
    "t = np.linspace(-2, 5, 100)\n",
    "\n",
    "# Solve the ODE with the adjusted initial condition\n",
    "y = odeint(model, initial_condition, t)\n",
    "\n",
    "# Plot the solution\n",
    "plt.plot(t, y, label='y(t)')\n",
    "plt.xlabel('Time (t)')\n",
    "plt.ylabel('y')\n",
    "plt.title('Solution of the Pulse Change ODE')\n",
    "plt.legend()\n",
    "plt.grid(True)\n",
    "plt.show()\n"
   ]
  },
