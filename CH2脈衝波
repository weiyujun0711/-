   "source": [
    "import numpy as np\n",
    "from scipy.integrate import odeint\n",
    "import matplotlib.pyplot as plt\n",
    "def func(y,t):\n",
    "    dydt = u(t) -y\n",
    "    return dydt\n",
    "def u(t):\n",
    "    if t>0:\n",
    "        return t\n",
    "    else:\n",
    "        return 0\n",
    "t = np.linspace(0,5,100)\n",
    "y = odeint(func, 0, t)\n",
    "plt.plot(t,y)\n",
    "plt.plot(t,[u(ti) for ti in t])\n",
    "plt.show()"
