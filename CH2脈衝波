   "source": [
    "import numpy as np\n",
    "from scipy.integrate import odeint\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "tau = 63.6\n",
    "k1 = 0.6364\n",
    "k2 = 0.3636\n",
    "T0 = 60\n",
    "Ts = 25\n",
    "\n",
    "def heating(T, t):\n",
    "    dTdt = (k1*Ti(t) + k2*Ts - T)/tau\n",
    "    return dTdt\n",
    "\n",
    "def Ti(t):\n",
    "    if t < 0:\n",
    "        return 80\n",
    "    elif t >= 0:\n",
    "        return 85\n",
    "\n",
    "# Time span\n",
    "t = np.linspace(-2, 300, 100)\n",
    "T = odeint(heating, T0, t)\n",
    "\n",
    "plt.plot(t, T, label='T(t)', color='green')\n",
    "plt.xlabel('Time (t)')\n",
    "plt.ylabel('Temperature (T)')\n",
    "plt.title('Time vs Temperature')\n",
    "plt.legend()\n",
    "plt.grid(True)\n",
    "plt.show()\n"
   ]
  },
