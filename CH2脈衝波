   "source": [
    "import numpy as np\n",
    "from scipy.integrate import odeint\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Define the system of differential equations of case one\n",
    "def model1(y, t):\n",
    "    dydt = u1(t) - y\n",
    "    return dydt\n",
    "\n",
    "# Define the input signal u(t) of case one\n",
    "def u1(t):\n",
    "    if 0 <= t <= 1:\n",
    "        return  t\n",
    "    elif 1 < t <= 2:\n",
    "        return  (2 - t)\n",
    "    else:\n",
    "        return 0\n",
    "    \n",
    "# Define the system of differential equations of case one\n",
    "def model2(y,t):\n",
    "    dydt = u2(t) - y\n",
    "    return dydt\n",
    "# Define the input signal u(t) of case one\n",
    "def u2(t):\n",
    "    if 0 <= t <= 5:\n",
    "        return 0\n",
    "    elif 5 <= t <= 6:\n",
    "        return (t-5)\n",
    "    elif 6 <= t <= 7:\n",
    "        return (7-t)\n",
    "    else:\n",
    "        return 0\n",
    "\n",
    "# Time span\n",
    "t = np.linspace(-2, 14, 1000)\n",
    "\n",
    "# Solve the system for each time span\n",
    "y1 = odeint(model1, 0, t)\n",
    "y2 = odeint(model2, 0, t)\n",
    "\n",
    "# Plot t vs u and t vs y in the same plot\n",
    "plt.plot(t, [u1(ti) for ti in t], label='u1(t)', color='blue')\n",
    "plt.plot(t, y1, label='y1(t)', color='green')\n",
    "plt.plot(t, [u2(ti) for ti in t], label='u2(t)', color='red')\n",
    "plt.plot(t, y2, label='y2(t)', color='orange')\n",
    "plt.xlabel('Time (t)')\n",
    "plt.ylabel('Input (u) / Output (y)')\n",
    "plt.title('Time vs Input and Output')\n",
    "plt.legend()\n",
    "plt.grid(True)\n",
    "plt.show()\n"
   ]
  },
